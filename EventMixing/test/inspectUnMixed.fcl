# Run the data product inspector on a file of unmixed events.
#
# $Id: inspectUnMixed.fcl,v 1.2 2012/03/02 17:16:22 gandr Exp $
# $Author: gandr $
# $Date: 2012/03/02 17:16:22 $

#include "fcl/minimalMessageService.fcl"
#include "fcl/standardProducers.fcl"

# Give this job a name.  
process_name : InspectDataProducts

# Start form an empty source
source : 
{ 
  module_type : RootInput
  fileNames   : [ "/mu2e/data/outstage/kutschke/72784/72784_0000/TTrackerBg_0000_data.root" ]
  maxEvents   : 20
}

services : 
{
  message : @local::default_message 

  TFileService : 
  {
    fileName : "readback.root"
  }

  GeometryService :
  {
    inputFile : "Mu2eG4/geom/geom_common.txt"
  }  

  ConditionsService :
  {
    conditionsfile : "Mu2eG4/test/conditions_01.txt"
  }  

  GlobalConstantsService : { inputFile : "Mu2eG4/test/globalConstants_01.txt" }


}

physics :
{
  producers:
  {  }


  analyzers:
  {

    inspect:
    {
      module_type            : InspectDataProducts
      generatorModuleLabel   : generate
      mixerModuleLabel       : g4run
    }

  } 

  e1 : [inspect]

  end_paths      : [e1]

}
