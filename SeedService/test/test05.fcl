# Test the seeds service.
#  Check range check works in predefined mode.
#
#

#include "Offline/fcl/messageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::mf_errorsOnly

   SeedService: {
      policy            : "preDefinedOffset"
      baseSeed          :     1
      maxUniqueEngines  :     6
      checkRange        :  true
      verbosity         :     2
      endOfJobSummary   :  false

      stest01 : {
         a : 2
         b : 4
        }

      stest02 : {
         a : 6
         c : 8
      }
   }

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      testMode      : 1
      instanceNames : [ "a", "b" ]
    }

    stest02 : {
      module_type : SeedTest01
      testMode      : 1
      instanceNames : [ "a", "c" ]
    }

  }

  e1 : [stest01, stest02]

  end_paths      : [e1]

}
