# Test the seeds service.
#  Check that stest01.a and stest02.a are properly distinguished.
#
#
# $Id: test03.fcl,v 1.3 2012/02/10 16:26:42 gandr Exp $
# $Author: gandr $
# $Date: 2012/02/10 16:26:42 $

#include "minimalMessageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::default_message

  user : {
     SeedService: {
      policy            : "preDefinedOffset"
      baseSeed          :     1
      maxUniqueEngines  :    20
      checkRange        :  true
      verbosity         :     2
      endOfJobSummary   :  false

      stest01 : {
         a : 2
         b : 4
        }

      stest02 : {
         a : 6
         c : 8
      }
     }
  }

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      testMode      : 1
      instanceNames : [ "a", "b" ]
    }

    stest02 : {
      module_type : SeedTest01
      testMode      : 1
      instanceNames : [ "a", "c" ]
    }

  }

  e1 : [stest01, stest02]

  end_paths      : [e1]

}
