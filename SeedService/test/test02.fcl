# Test the seeds service: test preDefinedOffset mode
#
#

#include "fcl/messageService.fcl"

# Give this job a name.
process_name : SeedTest01

# Start form an empty source
source :
{
  module_type : EmptyEvent
  maxEvents : 1
}

services :
{
  message : @local::mf_errorsOnly

   SeedService: {
      policy            : "preDefinedOffset"
      baseSeed          :     1
      maxUniqueEngines  :    20
      checkRange        :  true
      verbosity         :     2
      endOfJobSummary   :  false

      stest01 : 2

      stest02 : {
         a : 4
         c : 6
      }
   }

}

physics :
{
  analyzers: {
    stest01 : {
      module_type : SeedTest01
      testMode      : 0
      instanceNames : [ "a", "b" ]
    }

    stest02 : {
      module_type : SeedTest01
      testMode      : 1
      instanceNames : [ "a", "c" ]
    }

  }

  e1 : [stest01, stest02]

  end_paths      : [e1]

}
