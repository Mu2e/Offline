BEGIN_PROLOG
# Remove the StrawHits from an input file.  This allows them to be remade
dropSH :
	 [ "keep *_*_*_*"
   ,"drop mu2e::StrawHits_*_*_*"
	 ,"drop mu2e::StrawHitMCTruthits_*_*_*"
	 ,"drop mu2e::StepPointMCart::Ptrss_*_*_*" ]

#
 
END_PROLOG
