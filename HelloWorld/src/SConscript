import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper(env);

mainlib = helper.make_mainlib ( [ ] )

helper.make_plugins( [ mainlib,
                       'mu2e_MCDataProducts',
                       'art_Framework_Core',
                       'art_Framework_Principal',
                       'art_Persistency_Common',
                       'art_Persistency_Provenance',
                       'art_Utilities',
                       'canvas',
                       'fhiclcpp',
                       'fhiclcpp_types',
                       'cetlib',
                       'cetlib_except',
                       'boost_filesystem',
                       'Core',
                     ] )

