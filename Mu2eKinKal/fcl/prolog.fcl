    SaveFullFit : true
    Extend : true
    #    ZSavePositions : [-1522.0,0.0,1522.0]
  }
  KKKINEMATICLINE : {
    SeedErrors : [36, 0.025,600, 1.0, 25.0,1.0] # //d0_ = 0, phi0_ = 1,z0_ = 2,cost_ = 3,t0_ = 4,mom_ = 5,
