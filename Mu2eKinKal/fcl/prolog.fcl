BEGIN_PROLOG
Mu2eKinKal : {
  # general configuration
  #
  KKMAT: {
    elements  : "Offline/TrackerConditions/data/ElementsList.data"
    isotopes  : "Offline/TrackerConditions/data/IsotopesList.data"
    materials : "Offline/TrackerConditions/data/MaterialsList.data"
    strawGasMaterialName  : "straw-gas"
    strawWallMaterialName : "straw-wall"
    strawWireMaterialName : "straw-wire"
    dahlLynchScatteringFraction : 0.995
  }
  KKMU2E: {
    PrintLevel : 0
    TPOCAPrecision : 1e-4 # mm
    UseCaloCluster : true
    AddMaterial : true
    StrawHitGroupDeltaT : 1.0 # ns
    StrawHitGroupLevel : "uniquepanel"
    CaloTrackerTimeOffset : -1.35 #ns
    CaloPositionResolution : 15.0 #mm
    CaloPropagationSpeed : 200.0 #mm/ns
    MinCaloClusterEnergy : 10.0 #MeV
    MaxCaloClusterDt : 5.0 # ns
    MaxCaloClusterDOCA : 50.0 # mm
    AddHitSelect : []
    AddHitReject : ["Dead"]
    MaxStrawHitDOCA : 4.0 # mm
    MaxStrawHitDt : 5.0 # ns
    StrawBuffer : 1 #
    MaxStrawDOCA : 2.5 # mm
  }

  KKSEEDFIT: {
    PrintLevel : 0
    MinNDOF : 1
    MaxNIter : 10
    Deweight : 1.0e6
    ConvergenceDeltaChisq : 0.1
    DivergenceDeltaChisq : 10.0
    DeltaParams : 1e6 # chisquared
    BFieldCorrection : false
    ProcessEnds : false
    BCorrTolerance : 1e-4 # momemntum fraction
    MetaIterationSettings : [
      [ 5.0, 0 ],
      [ 2.0, 0 ],
      [ 1.0, 0 ]
    ]
    NullStrawHitUpdaterSettings : [
      [ 10.0],
      [ 5.0 ],
      [ 4.0 ]
    ]
    DOCAStrawHitUpdaterSettings : []
    CombinatoricStrawHitUpdaterSettings : []
    StrawXingUpdaterSettings : [
      [-1.0, 10., 10.0 ],
      [-1.0, 5., 5.0 ],
      [-1.0, 4., 4.0 ]
    ]
  }

  KKSEEDEXT: {
    PrintLevel : 0
    MinNDOF : 1
    MaxNIter : 10
    Deweight : 1.0e6
    ConvergenceDeltaChisq : 0.1
    DivergenceDeltaChisq : 10.0
    DeltaParams : 1e6 # chisquared
    BFieldCorrection : false
    ProcessEnds : false
    BCorrTolerance : 1e-4 # momemntum fraction
    MetaIterationSettings : [
      [ 1.0, 0 ]
    ]
    NullStrawHitUpdaterSettings : [
      [ 4.0 ]
    ]
    DOCAStrawHitUpdaterSettings : []
    CombinatoricStrawHitUpdaterSettings : []
    StrawXingUpdaterSettings : [
      [-1.0, 4.0, 4.0 ]
    ]
  }

  KKDRIFTFIT: {
    PrintLevel : 0
    MinNDOF : 1
    MaxNIter : 10
    Deweight : 1.0e6
    ConvergenceDeltaChisq : 0.1
    DivergenceDeltaChisq : 10.0
    DeltaParams : 1e6 # chisquared
    BFieldCorrection : true
    ProcessEnds : false
    BCorrTolerance : 1e-4 # momemntum fraction
    MetaIterationSettings : [
      [ 5.0, 0 ],
      [ 2.0, 0 ],
      [ 1.0, 0 ]
    ]
    NullStrawHitUpdaterSettings : [
      [ 10.0],
      [ 5.0 ],
      [ 4.0 ]
    ]
    DOCAStrawHitUpdaterSettings : []
    CombinatoricStrawHitUpdaterSettings : []
    StrawXingUpdaterSettings : [
      [-1.0, 10.0, 100.0 ],
      [-1.0, 5.0,  100.0 ],
      [-1.0, 4.0, 100.0 ]
    ]
  }


  KKDRIFTEXT: {
    PrintLevel : 0
    MinNDOF : 1
    MaxNIter : 10
    Deweight : 1.0e6
    ConvergenceDeltaChisq : 1.0
    DivergenceDeltaChisq : 50.0
    DeltaParams : 1e6 # chisquared
    BFieldCorrection : true
    ProcessEnds : false
    BCorrTolerance : 1e-4 # momemntum fraction
    MetaIterationSettings : [
      [ 1.0, 0 ],
      [ 2.0, 1 ],
      [ 1.0, 1 ],
      [ 0.0, 1 ]
    ]
    NullStrawHitUpdaterSettings : [
      [ 5.0 ]
    ]
    DOCAStrawHitUpdaterSettings : [
      [ 6.0, 0.5, 5.0 ],
      [ 6.0, 0.5, 5.0 ],
      [ 6.0, 0.5, 5.0 ]
    ]
    CombinatoricStrawHitUpdaterSettings : []
    StrawXingUpdaterSettings : [
      [-1.0, 5.0, 5.0 ],
      [0.5, 4.0, 4.0 ],
      [0.4, 3.0, 4.0 ],
      [0.3, 3.0, 4.0 ]
    ]
  }

  KKLOOPHELIX : {
    SeedErrors : [5.0, 5.0, 5.0, 5.0, 0.02, 5.0] # R(mm), Lambda(mm), Cx(mm), Cy(mm), phi0, t0 (ns)
