                                  'mu2e_ConfigTools',
                                  'mu2e_Mu2eInterfaces',
                                  'mu2e_DataProducts',
                                  'art_Framework_Core',
                                  'art_Framework_Services_Registry',
                                  'CLHEP',
                                  'cetlib',
                                  'cetlib_except',
                                  ] )

helper.make_plugins( [ mainlib,
                       'mu2e_TrackerConditions',
                       'mu2e_Mu2eBTrk',
                       'mu2e_Mu2eInterfaces',
                       'mu2e_ConfigTools',
                       'art_Framework_Principal',
                       'art_Persistency_Provenance',
                       'art_Framework_Services_Registry',
                       'art_root_io_tfile_support',
                       'art_Utilities',
                       'canvas',
                       'MF_MessageLogger',
                       'fhiclcpp',
                       'fhiclcpp_types',
                       'tbb',
                       'canvas',
                       'cetlib',
                       'cetlib_except',
                       'boost_filesystem',
                       ] )


