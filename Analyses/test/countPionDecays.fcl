# Example of running CountPionDecays
#

#include "Offline/fcl/minimalMessageService.fcl"

process_name : Count

source : {
  module_type : RootInput
  fileNames   : [ "beamlineData_02.root" ]
  maxEvents   : -1
}

services : {
  message : @local::default_message
}

physics : {

  analyzers: {

    count : {
      module_type   : CountPionDecays
      g4ModuleLabel : "g4run"
    }

  }

  e1 : [count]

  end_paths      : [e1]

}
