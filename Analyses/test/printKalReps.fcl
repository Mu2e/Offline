}

physics.analyzers.printCPRDeM : {
    module_type    : KalRepsPrinter
    tracksTag      : "CalPatRecDem:DeM"
    kalDiag        : @local::KalDiagReadCD3
    outputFilename : "CalPatRec.txt"

}

physics.analyzers.testTrkPatRecType1 : {
