# Configuration file for PS Vacuum Readback
#
# Readback the output of data_03.root; make histograms and printout.
#
# $Id: psVacuum_readback.fcl,v 1.1 2012/06/22 18:14:56 youzy Exp $
# $Author: youzy $
# $Date: 2012/06/22 18:14:56 $
#
# Original author Zhengyun You

#include "fcl/minimalMessageService.fcl"
#include "fcl/standardProducers.fcl"

# Give this job a name.  
process_name : VDReadBack01

# Start form an empty source
source : 
{ 
  module_type : RootInput
  fileNames   : [ "data_03.root" ]
  maxEvents : -1
}

services : 
{
  message : @local::default_message 

  TFileService : 
  {
    fileName : "psVacuum_readback.root"
  }

  GeometryService :
  {
    inputFile : "Mu2eG4/geom/geom_common.txt"
  }  

  ConditionsService :
  {
    conditionsfile : "Mu2eG4/test/conditions_01.txt"
  }  

  GlobalConstantsService : { inputFile : "Mu2eG4/test/globalConstants_01.txt" }

}

physics :
{
  producers:
  {  }


  analyzers:
  {

    checkhits:
    {
      module_type            : ReadPSVacuum 
      generatorModuleLabel   : generate
      g4ModuleLabel          : g4run
      minimumEnergy          : 0.000
      maxPrint               : 10
      diagLevel              : -1
      savePDG                : [11, -11, 13, -13, 22, 111, 211, -211, 321, -321, 2112, 2212, -2212]
    }

  } 

  e1 : [checkhits]

  end_paths      : [e1]

}

