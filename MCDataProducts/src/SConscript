import os
Import('env')
Import('mu2e_helper')

helper=mu2e_helper();

mainlib = helper.make_mainlib ( [ 'mu2e_RecoDataProducts',
                                  'mu2e_DataProducts',
                                  'art_Framework_Core',
                                  'art_Framework_Core',
                                  'art_Utilities',
                                  'art_Framework_Services_Registry',
                                  'art_Persistency_Provenance',
                                  'cetlib',
                                  'Reflex',
                                  'Core',
                                ] )

helper.make_plugins( [ mainlib,
                     ] )

helper.make_dict_and_map( [ mainlib,
                          ],
                          [ '-fvar-tracking-assignments-toggle'] )

